import requests import random import string

API_URL = "https://api.mail.tm"

def generate_random_name(): return ''.join(random.choices(string.ascii_lowercase + string.digits, k=10))

def create_account(): username = generate_random_name() password = generate_random_name() + "A1!" domain = "@cloud-mail.top" email = username + domain

response = requests.post(
    f"{API_URL}/accounts",
    json={"address": email, "password": password}
)

if response.status_code == 201:
    print("==============================")
    print(f"‚úÖ Email: {email}")
    print(f"üîë Password: {password}")
    print("==============================")
else:
    print("==============================")
    print(f"‚ùå –û—à–∏–±–∫–∞: {response.status_code}")
    print(response.text)
    print("==============================")

def main(): while True: command = input("–ù–∞–ø–∏—à–∏ .gmail –¥–ª—è —Å–æ–∑–¥–∞–Ω–∏—è –≤—Ä–µ–º–µ–Ω–Ω–æ–π –ø–æ—á—Ç—ã: ").strip().lower() if command == ".gmail": create_account() else: print("üö´ –ù–µ–∏–∑–≤–µ—Å—Ç–Ω–∞—è –∫–æ–º–∞–Ω–¥–∞. –ù–∞–ø–∏—à–∏ .gmail")

if name == "main": main()

